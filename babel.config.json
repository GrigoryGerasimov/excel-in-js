{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "chrome": "108",
                    "safari": "16",
                    "edge": "108",
                    "firefox": "108",
                    "node": "current"
                },
                "useBuiltIns": "usage",
                "corejs": "3.9.0",
                "loose": true
            }
        ]
    ],
    "plugins": ["@babel/plugin-proposal-class-properties"]
}